<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.zway.fic.kanban.dao.GuideTemplateDao">

    <select id="getAll" resultType="top.zway.fic.base.entity.doo.GuideTemplateDO">
        SELECT
            `index`,
            `type`,
            content,
            color
        FROM
            guide_template
        ORDER BY
            `index` ASC
    </select>
</mapper>